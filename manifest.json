{
  "manifest_version": 3,
  "name": "CoolRefs",
  "version": "0.0.1",
  "icons": {
    "16": "images/hello_extension.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "page_action": {
    "default_title": "Download tweet images, CoolRefs style",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}